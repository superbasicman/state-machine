{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/s/([^/]+)",
      "dest": "/api/session/[token].js?token=$1"
    },
    {
      "src": "/api/events/([^/]+)",
      "dest": "/api/events/[token].js?token=$1"
    },
    {
      "src": "/api/history/([^/]+)",
      "dest": "/api/history/[token].js?token=$1"
    },
    {
      "src": "/api/submit/([^/]+)",
      "dest": "/api/submit/[token].js?token=$1"
    },
    {
      "src": "/api/config/([^/]+)",
      "dest": "/api/config/[token].js?token=$1"
    },
    {
      "src": "/api/ws/cli",
      "dest": "/api/ws/cli.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
